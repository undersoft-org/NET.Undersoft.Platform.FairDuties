@code {
    private Foo Model { get; set; } = new Foo() { Name = "" };
    private static List<string> StaticItems => new() { "1", "12", "123", "1234", "12345", "123456", "abc", "abcdef", "ABC", "aBcDeFg", "ABCDEFG" };
}
