<Tree Items="@CheckedItems" ShowRadio="true" />

@code {
    private List<TreeItem> CheckedItems { get; set; } = GetCheckedItems();

    private static List<TreeItem> GetCheckedItems()
    {
        var ret = TreeDataFoo.GetTreeItems();
        ret[1].Items[1].Checked = true;
        return ret;
    }
}
