  "Title": "Undersoft Operational Data Provider API",
  "Description": "Undersoft Operational Data Provider API",

  "Repository": {
    "Endpoints": {
      "Undersoft.ODP.Infra.Data.Base.Contexts.EntryDb": {
        "ConnectionString": "Host=localhost; Port=5432; Database=ODP.Entry; MemberId=postgres;Password=V00dkaPL; Pooling=true;",
        "EndpointProvider": "PostgreSql",
        "PoolSize": 128
      },
      "Undersoft.ODP.Infra.Data.Base.Contexts.EventDb": {
        "ConnectionString": "Host=localhost; Port=5432; Database=ODP.Event; MemberId=postgres;Password=V00dkaPL; Pooling=true;",
        "EndpointProvider": "PostgreSql",
        "PoolSize": 128
      },
      "Undersoft.ODP.Infra.Data.Base.Contexts.ReportDb": {
        "ConnectionString": "Host=localhost; Port=5432; Database=ODP.Report; MemberId=postgres;Password=V00dkaPL; Pooling=true;",
        "EndpointProvider": "PostgreSql",
        "PoolSize": 128
      }
    }
  },
  "StoreRoutes": {
    "EntryStore": "dataentry",
    "EventStore": "event",
    "ReportStore": "datareport",
    "CqrsStore": "data",
    "OpenEventStore": "openevent",
    "OpenCqrsStore": "opendata",
    "GrpcEventStore": "eventstream",
    "GrpcCqrsStore": "datastream",
    "RestEventStore": "eventcrud",
    "RestCqrsStore": "datacrud"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    },
    "OpenTelemetry": {
      "IncludeFormattedMessage": true,
      "IncludeScopes": true,
      "ParseStateValues": true
    }
  },
  "DataCache": {
    "Hours": "0",
    "Minutes": "45"
  },
  "IdentityServer": {
    "BaseUrl": "https://localhost:44310",
    "ApiName": "Undersoft Operational Data Provider API",
    "ApiVersion": "v1",
    "ApiBaseUrl": "https://localhost:31283",
    "OidcSwaggerUIClientId": "odp.openapi",
    "OidcApiName": "odp.api",
    "Scopes": [
      "odp.api.rw",
      "idp.api.rw",
      "aep.api.rwx"
    ],
    "Roles": [
      "User",
      "Leader",
      "Manager",
      "Administrator"
    ],
    "AdministrationRole": "Administrator",
    "RequireHttpsMetadata": false,
    "CorsAllowAnyOrigin": true,
    "CorsAllowOrigins": []
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "ServiceName": "io.undersoft.odp.api",
  "AllowedHosts": "*",
  "UseTracingExporter": "jeager",
  "UseMetricsExporter": "prometheus",
  "UseLogExporter": "console",
  "HistogramAggregation": "explicit",
  "Jaeger": {
    "AgentHost": "localhost",
    "AgentPort": 6831,
    "Endpoint": "http://localhost:14268",
    "Protocol": "UdpCompactThrift"
  },
  "AspNetCoreInstrumentation": {
    "RecordException": "true"
  }
}
